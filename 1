#include <stdio.h>

int main() {
  printf( "- - - - - - - - - - - - - - - - - \n" );
  printf( "      N O T A   L E G A L\n" );
  printf( "- - - - - - - - - - - - - - - - - \n" );
  printf( "- Item            Qtd  Prc   Valor \n" );
  printf( "%s %3i %f %f\n", "Caneta Azul",     2,  2.0,  4.0 );
  printf( "%s %3i %f %f\n", "Borracha",        1,  5.00,   5.00 );
  printf( "%s %3i %f %f\n", "Resma de papel",  1, 10.0,  10.00 );
  printf( "- - - - - - - - - - - - - - - - - \n" );
  printf( "  TOTAL: R$ 19.00\n");
  return 0;
}
